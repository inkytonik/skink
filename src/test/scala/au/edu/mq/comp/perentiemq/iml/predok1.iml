def predok1(a : Bool): (b : Bool) {

    // Single operators
    assert (a || b);
    assert (a && b);
    assert (c == d);
    assert (c != d);
    assert (c <= d);
    assert (c < d);
    assert (c >= d);
    assert (c > d);
    assert (!b);

    // Literals
    assert (false);
    assert (true);

    // Precedence
    assert (a || b && c);
    assert (a && b || c);

    // Associativity
    assert (b || c || d);
    assert (b && c || d);
    assert (b || c && d);
    assert (b && c && d);

    // Parenthesisation
    assert ((b || c) && d);
    assert (b && (c || d));
    assert (b || (c || d));
    assert ((c == d) || (d < c));

}
