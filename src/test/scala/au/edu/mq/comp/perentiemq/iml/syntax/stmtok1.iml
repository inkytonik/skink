def stmtok1(a) : (b) {

    // Assert
    assert (a == b);

    // Assign
    c = 1;
    a = b + c;
    a, b = 10;
    a, b = c, d, e;

    // Assume
    assume (a == b);

    // Call
    a = f(a);
    b = g(a, b);

    // If
    if (a == b) a = 1; else a = 2;
    if (a == b)
        if (b == 1)
            a = 1;
        else
            a = 2;
    else
        a = 3;

    // Goto
    goto label;

    // Label
    label : a = 2;

    // Skip
    skip;

    // While
    while (a != b)
        a = a + 1;
    while (a < 10) {
        a = a + 1;
        b = b - 1;
    }

}
